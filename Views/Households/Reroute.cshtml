

@{
    ViewBag.Title = "Reroute";
}

<h4></h4>

@model FP.Models.Household

@{
    ViewBag.Title = "Details";
}


<div class="modal-content">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title"><img src="~/img/fp.png" /></h4>
    </div>
    <div class="modal-body">
        <div class="container">
            <div class="row">
            </div>
        </div>
        <div class="logo">
            <h1>Financial<span class="color bold">Planner</span></h1>
            <p class="meta">Your one-stop personal finance-tracking resource</p>
    
        @{ var temporaryhouseholdmessage = TempData["message"];}
        @{ var temporarybudgetsmessage = TempData["budgetmessage"];}
        @{ var AlreadyinHousehold = TempData["AlreadyinHouseholdmessage"];}
         @{ var NoHousehold = TempData["Nohouseholdmessage"];}
        @{ var NotLoggedInmessage = TempData["NotLoggedInmessage"];}
        @{ var NotLoggedIn2message = TempData["NotLoggedIn2message"];}
            @NoHousehold
        @NotLoggedIn2message
        @NotLoggedInmessage
        @temporaryhouseholdmessage
        @temporarybudgetsmessage
        @AlreadyinHousehold
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
    </div>
</div>
</div>